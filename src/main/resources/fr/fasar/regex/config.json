{
  "extract": {
    "plu": [ "^(\\d+)" ],
    "lib": [ "^\\d+\t\t([^\\t]+)" ],
    "energie": ["(?i)(\\d* ?\\d+(?:,|\\.)?\\d*) ?kj"],
    "calories": ["(?i)(\\d+(?:,|\\.)?\\d*) ?kcal"],
    "glucide": ["(?i)Glucide[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"],
    "lipide": ["(?i)Matieres? ?grasse[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)", "(?i)Graisse[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"],
    "acides gras satures": ["(?i)gras sature[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"],
    "sucre": ["(?i)sucre[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"],
    "proteine": ["(?i)Proteine[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"],
    "sel": ["(?i)sel[^\\d]{0,4}(\\d+(?:,|\\.)?\\d*)"]
  },
  "numerical": ["energie", "calorie", "glucide", "lipide", "acides gras satures", "sucre", "proteine", "sel"],
  "charset": "UTF8"

}