{
  "extract": {
    "plu": [ "^(\\d+)" ],
    "lib": [ "^\\d+,,([^,]+)" ],
    "energie": ["(?i)(\\d* ?\\d+(?:,|\\.)?\\d*) ?kj"],
    "calories": ["(?i)(\\d+(?:,|\\.)?\\d*) ?kcal"],
    "lipide": ["(?i)Matieres? grasse[^\\d]*(\\d+(?:,|\\.)?\\d*)", "(?i)Graisse[^\\d]*(\\d+(?:,|\\.)?\\d*)"],
    "glucide": ["(?i)Glucide[^\\d]*(\\d+(?:,|\\.)?\\d*)"],
    "acides gras satures": ["(?i)gras sature[^\\d]*(\\d+(?:,|\\.)?\\d*)"],
    "sucre": ["(?i)sucre[^\\d]*(\\d+(?:,|\\.)?\\d*)"],
    "proteine": ["(?i)Proteine[^\\d]*(\\d+(?:,|\\.)?\\d*)"],
    "sel": ["(?i)sel[^\\d]*(\\d+(?:,|\\.)?\\d*)"]
  },
  "charset": "UTF8"

}